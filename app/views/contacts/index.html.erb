<h1>Contacts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>Number</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.name %></td>
        <td><%= contact.age %></td>
        <td><%= contact.number %></td>
        <td><%= contact.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>
